{
  "name": "react_redux_demo",
  "description": "A Simple Demo developed by React & Redux",
  "author": "xiongsheng",
  "license": "MIT",
  "version": "0.1.0",
  "scripts": {
    "start": "cross-env NODE_ENV=dev bizdp server",
    "build": "cross-env NODE_ENV=production webpack --progress --colors",
    "rm": "rimraf ./dist ",
    "server:bizdp": "cross-env NODE_ENV=dev bizdp server"
  },
  "devDependencies": {
    "babel-core": "6.3.26",
    "babel-eslint": "5.0.0-beta6",
    "babel-loader": "6.2.0",
    "babel-plugin-antd": "0.1.3",
    "babel-plugin-lodash": "1.0.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "6.3.13",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-react-hmre": "1.0.1",
    "babel-preset-stage-0": "6.3.13",
    "babel-runtime": "6.3.19",
    "css-loader": "0.23.1",
    "eslint": "1.10.3",
    "eslint-plugin-babel": "3.0.0",
    "eslint-plugin-react": "3.13.1",
    "extract-text-webpack-plugin": "0.8.2",
    "file-loader": "0.8.4",
    "html-webpack-plugin": "2.22.0",
    "rimraf": "2.4.3",
    "style-loader": "0.12.3",
    "webpack": "1.12.9",
    "webpack-dev-middleware": "1.4.0",
    "webpack-hot-middleware": "2.12.0"
  },
  "dependencies": {
    "@bizfe/biz-dp": "0.1.0",
    "antd": "2.0.1",
    "assets-webpack-plugin": "3.4.0",
    "es6-promise": "3.1.2",
    "font-awesome": "4.6.3",
    "history": "1.13.1",
    "lodash": "3.10.1",
    "moment": "2.10.6",
    "normalizr": "1.0.0",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "react-router-redux": "4.0.0",
    "redux": "3.5.2",
    "redux-logger": "0.0.3",
    "redux-thunk": "2.1.0"
  },
  "bizdp": {
    "npm": {
      "command": "npm",
      "registry": "http://registry.npm.sogou"
    },
    "build": "gulp",
    "server": {
      "command": "webpack-dev-server",
      "options": {
        "inline": true,
        "hot": true,
        "host": "0.0.0.0",
        "colors": true,
        "progress": true,
        "display-modules": true
      }
    },
    "validate": {
      "command": "eslint",
      "options": {},
      "src": "src"
    }
  }
}
